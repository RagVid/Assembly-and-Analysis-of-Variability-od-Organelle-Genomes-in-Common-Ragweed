  "ChNAm1" = brewer.pal(9, "Set1")[1],  # First color in Set1 palette
  "ChNAm2" = brewer.pal(9, "Set1")[2],  # Second color in Set1 palette
  "ChNAm3" = brewer.pal(9, "Set1")[4],  # etc.
